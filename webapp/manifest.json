{
	"_version": "1.12.0",
	"sap.app": {
		"id": "gdsd.CW09",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZDSD_CRM_UI5_APP_SRV": {
				"uri": "/sap/opu/odata/sap/ZDSD_CRM_UI5_APP_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "gdsd.CW09.view.WorkSpace",
			"type": "XML",
			"async": true,
			"id": "WorkSpace"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "gdsd.CW09.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZDSD_CRM_UI5_APP_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "gdsd.CW09.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"viewLevel": 1
			},
			"routes": [
				{
					"name": "WorkSpace",
					"pattern": "WorkSpace",
					"target": [
						"WorkSpace"
					]
				},
				{
					"name": "TaskDetails",
					"pattern": "TaskDetails",
					"target": [
						"TaskDetails"
					]
				}
			],
			"targets": {
				"TaskDetails": {
					"viewType": "XML",
					"viewName": "TaskDetails"
				},
				"WorkSpace": {
					"viewType": "XML",
					"viewName": "WorkSpace"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zcw09/webapp",
		"_version": "1.1.0"
	}
}